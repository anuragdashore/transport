<h1>Your Data</h1>

<table class="table table-striped table-bordered">
	<thead>
		<th>id</th>
		<th>User_Name</th>
		<th>Contact_no</th>
		<th>Email</th>
		<th>Age</th>
		<th>Gender</th>
		<th>Role</th>
	</thead>
	<tbody>
		<tr>
			<td>
				<%= @user.id %>
				</td>
			<td>
				<%= @user.name %>
			</td>
			<td>
				<%= @user.contact %>
			</td>
			<td>
				<%= @user.email %>
			</td>
			<td>
				<%= @user.age %>
			</td>
			<td>
				<%= @user.gender %>
			</td>
			<td>
				<%= @user.role_id %><br>
			</td>
		</tr>
	</tbody>
</table><br>
