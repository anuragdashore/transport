<h2>Users</h2>

<table style="width:40%", border="1px">
	<thead>
		<th>id</th>
		<th>User_Name</th>
		<th>Contact_no</th>
		<th>Age</th>
		<th>Gender</th>
		<th>Role</th>
	</thead>
	<tbody>
		<tr>
			<% @users.each do |user| %>
				<td>
					<%= user.id %>
				</td>
				<td>
					<%= link_to user.name, user %>
				</td>
				<td>
					<%= user.contact %>
				</td>
				<td>
					<%= user.age %>
				</td>
				<td>
					<%= user.gender %>
				</td>
				<td>
					<%= user.role %><br>
				</td>
		</tr>
			<% end %>
	</tbody>
</table><br>	
