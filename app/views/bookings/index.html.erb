<h1>Your Bookings</h1>
<% if current_user.has_role? :manager 
	@manager_booking.each do |booking| %>
	<div class="me-5">
		<table class="table table-striped table-bordered">
			<tr>
				<th>user_id</th>
				<th>pickup_address</th>
				<th>pickup_city</th>
				<th>pickup_state</th>
				<th>pickup_date</th>
				<th>pickup_timing</th>
				<th>deliver_address</th>
				<th>deliver_city</th>
				<th>deliver_state</th>
				<th>Show Bookings</th>
			</tr>
				<tr>
					<td>
						<%= booking.user_id %>
					</td>
					<td>
						<%= booking.pickup_address %>
					</td>
					<td>
						<%= booking.pickup_city %>
					</td>
					<td>
						<%= booking.pickup_state %>
					</td>
					<td>
						<%= booking.pickup_date %>
					</td>
					<td>
						<%= booking.pickup_timing %>
					</td>
					<td>
						<%= booking.deliver_address %>
					</td>
					<td>
						<%= booking.deliver_city %>
					</td>
					<td>
						<%= booking.deliver_state %>
					</td>
					<td>
						<%= link_to "Show", booking, class: "btn btn-primary" %>
					</td>
				</tr>
		</table>
	</div>
	<% end %>
	<p> <%= paginate @manager_booking %> </p>
	<% else @bookings.each do |booking| %>
		<div class="me-5">
			<table class="table table-striped table-bordered">
				<thead>
					<th>pickup_address</th>
					<th>pickup_city</th>
					<th>pickup_state</th>
					<th>pickup_date</th>
					<th>pickup_timing</th>
					<th>deliver_address</th>
					<th>deliver_city</th>
					<th>deliver_state</th>
					<th>Show Bookings</th>
				</thead>
					<tr>
						<td>
							<%= booking.pickup_address %>
						</td>
						<td>
							<%= booking.pickup_city %>
						</td>
						<td>
							<%= booking.pickup_state %>
						</td>
						<td>
							<%= booking.pickup_date %>
						</td>
						<td>
							<%= booking.pickup_timing %>
						</td>
						<td>
							<%= booking.deliver_address %>
						</td>
						<td>
							<%= booking.deliver_city %>
						</td>
						<td>
							<%= booking.deliver_state %>
						</td>
						<td>
							<%= link_to "Show", booking, class: "btn btn-primary" %>
						</td>
					</tr>
				</tbody>
			</table>
		</div>
	<% end %>
	<p> <%= paginate @bookings %> </p>
<% end %>